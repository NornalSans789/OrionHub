local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/thanhdat4461/OrionMoblie/main/source')))()

local Window = OrionLib:MakeWindow({Name = "Copy Gui", HidePremium = false, SaveConfig = false, ConfigFolder = "OrionHub", IntroText = "Loading..."})

local Tab = Window:MakeTab({
	Name = "Tab Main",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddParagraph("Current Position Is:", tostring(game.Players.LocalPlayer.Character.HumanoidRootPart.Position))

Tab:AddButton({
	Name = "Copy Current Position",
	Callback = function()
	setclipboard(tostring(game.Players.LocalPlayer.Character.HumanoidRootPart.Position))
  	end    
})

Tab:AddButton({
	Name = "Copy PlaceId",
	Callback = function()
	setclipboard(tostring(game.PlaceId))
  	end    
})

Tab:AddButton({
	Name = "Fly Gui V3",
	Callback = function()
	loadstring(game:HttpGet("https://raw.githubusercontent.com/XNEOFF/FlyGuiV3/main/FlyGuiV3.txt"))()
  	end    
})

local CreditTab = Window:MakeTab({
        Name = "Tab Credit",
        Icon = "rbxassetid://4483345998",
        PremiumOnly = false
})

local CreditOwnerSection = CreditTab:AddSection({
        Name = "Script Owner"
})

CreditTab:AddLabel("By @SansYT789(Youtube)")