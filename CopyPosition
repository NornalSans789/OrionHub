local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/thanhdat4461/OrionMoblie/main/source')))()

local Window = OrionLib:MakeWindow({Name = "Copy Gui", HidePremium = false, SaveConfig = false, ConfigFolder = "OrionHub", IntroText = "Loading..."})

local currentpos = game.Players.LocalPlayer.Character.HumanoidRootPart.Position

while wait() do
pcall(function()
currentpos = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
end
end)

local Tab = Window:MakeTab({
	Name = "Tab Main",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddParagraph("Current Position:",tostring(currentpos))

Tab:AddButton({
	Name = "Copy Pos",
	Callback = function()
	setclipboard(tostring(currentpos))
      		print("Copy Success!")
  	end    
})

Tab:AddButton({
	Name = "Copy PlaceId",
	Callback = function()
	setclipboard(tostring(game.PlaceId))
      		print("Copy Success!")
  	end    
})

local CreditTab = Window:MakeTab({
        Name = "Tab Credit",
        Icon = "rbxassetid://4483345998",
        PremiumOnly = false
})

local CreditOwnerSection = CreditTab:AddSection({
        Name = "Script Owner"
})

CreditTab:AddLabel("By @SansYT789(Youtube)")