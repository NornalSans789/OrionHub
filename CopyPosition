local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/thanhdat4461/OrionMoblie/main/source')))()

local Window = OrionLib:MakeWindow({Name = "Copy Gui", HidePremium = false, SaveConfig = false, ConfigFolder = "OrionHub", IntroText = "Đang tải..."})

local newpos = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
local oldpos = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
local positiontextlabel = oldpos
local placeid = game.PlaceId

spawn(function()
while wait() do
if newpos ~= oldpos then
pcall(function()
positiontextlabel = newpos
end)
end
end
end)

local Tab = Window:MakeTab({
	Name = "Tab Main",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddParagraph("Toạ độ hiện tại là:", tostring(positiontextlabel))

Tab:AddButton({
	Name = "Sao chép toạ độ hiện tại",
	Callback = function()
	setclipboard(tostring(newpos))
  	end    
})

Tab:AddButton({
	Name = "Sao chép số id của map",
	Callback = function()
	setclipboard(tostring(placeid))
  	end    
})

local CreditTab = Window:MakeTab({
        Name = "Tab Credit",
        Icon = "rbxassetid://4483345998",
        PremiumOnly = false
})

local CreditOwnerSection = CreditTab:AddSection({
        Name = "Chủ sở hữu script"
})

CreditTab:AddLabel("Bởi @SansYT789(Youtube)")