local Window = OrionLib:MakeWindow({Name = "Nightlight", HidePremium = false, SaveConfig = false, ConfigFolder = "OrionHub", IntroText = "Loading Hub..."})
local hightlight = Instance.new("Highlight")
local rs = game:GetService("RunService")
getgenv().selectnote = nil

hightlight.Archivable = true
hightlight.Adornee = nil
hightlight.Parent = nil
hightlight.Name = "Hightlight"
hightlight.Enabled = true
hightlight.FillColor = Color3.new(255,0,0)
hightlight.FillTransparency = 0.5
hightlight.OutlineColor = Color3.new(255,255,255)
hightlight.OutlineTransparency = 0

local MainTab = Window:MakeTab({
	Name = "Tab Main",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local MainSection = MainTab:AddSection({
	Name = "Esp Section"
})

MainTab:AddToggle({
	Name = "Coins esp",
	Default = false,
	Callback = function(Value)
getgenv().EspCoin = Value

rs.RenderStepped:Connect(function()
if EspCoin == true then
for i,v in pairs(game:GetService("Workspace").House.Coins:GetChildren()) do
if not v:FindFirstChild("hilicoin") then
local hilicoin = hightlight:Clone()
hilicoin.Name = "hilicoin"
hilicoin.Parent = v
hilicoin.Adornee = v
hilicoin.FillColor = Color3.fromRGB(255,204,0)
end
end
else
if hilicoin ~= nil then
hilicoin:Destroy()
end
end
end)
	end    
})

MainTab:AddToggle({
	Name = "Matchs esp",
	Default = false,
	Callback = function(Value)
getgenv().EspMatch = Value

rs.RenderStepped:Connect(function()
if EspMatch == true then
for i,v in pairs(game:GetService("Workspace").Matches:GetChildren()) do
if not v:FindFirstChild("hilimatch") then
local hilimatch = hightlight:Clone()
hilimatch.Name = "hilimatch"
hilimatch.Parent = v
hilimatch.Adornee = v
hilimatch.FillColor = Color3.fromRGB(102,255,51)
end
end
else
if hilimatch ~= nil then
hilimatch:Destroy()
end
end
end)
	end    
})

MainTab:AddToggle({
	Name = "Monsters esp",
	Default = false,
	Callback = function(Value)
getgenv().EspBeta = Value

rs.RenderStepped:Connect(function()
if EspBeta == true then
for i,v in pairs(game:GetService("Workspace"):GetChildren()) do
if not v:FindFirstChild("hilibeta") and v.Name == "Beta" then
local hilibeta = hightlight:Clone()
hilibeta.Name = "hilibeta"
hilibeta.Parent = v
hilibeta.Adornee = v
end
end
else
if hilibeta ~= nil then
hilibeta:Destroy()
end
end
end)
	end    
})

MainTab:AddToggle({
	Name = "Notes esp",
	Default = false,
	Callback = function(Value)
getgenv().EspNote = Value

rs.RenderStepped:Connect(function()
if EspNote == true then
for i,v in pairs(game:GetService("Workspace").Notes:GetChildren()) do
if not v:FindFirstChild("hilinote") then
local hilinote = hightlight:Clone()
hilinote.Name = "hilinote"
hilinote.Parent = v
hilinote.Adornee = v
hilinote.FillColor = Color3.fromRGB(204,102,0)
end
end
else
if hilinote ~= nil then
hilinote:Destroy()
end
end
end)
	end    
})

MainTab:AddToggle({
	Name = "Players esp",
	Default = false,
	Callback = function(Value)
getgenv().EspPlayer = Value

rs.RenderStepped:Connect(function()
if EspPlayer == true then
for i,v in pairs(game:GetService("Players"):GetPlayers()) do
if not v:FindFirstChild("hiliplayer") then
local hiliplayer = hightlight:Clone()
hiliplayer.Name = "hiliplayer"
hiliplayer.Parent = v.Character
hiliplayer.Adornee = v.Character
hiliplayer.FillColor = Color3.fromRGB(0,128,255)
end
end
else
if hiliplayer ~= nil then
hiliplayer:Destroy()
end
end
end)
	end    
})

