local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/thanhdat4461/OrionMoblie/main/source')))()

-- local linh tinh
local HMRP = game.Players.LocalPlayer.Character.HumanoidRootPart
local HM = game.Players.LocalPlayer.Character.Humanoid

if game.PlaceId == 15240723287 then
OrionLib:MakeNotification({
	Name = "Script Loading...",
	Content = "Script has support your map: Secret Staycation",
	Image = "rbxassetid://4483345998",
	Time = 5
})
wait(0.15)
local Window = OrionLib:MakeWindow({Name = "Secret Staycation", HidePremium = false, SaveConfig = false, ConfigFolder = "OrionHub", IntroText = "Loading Hub..."})

-- local linh tinh :)
local playerdrop = {}
local player = game.Players:GetChildren()
getgenv().selectplr = nil
getgenv().selectstagetrain = nil
getgenv().selectplace = nil

-- function
for i,v in pairs(player) do
table.insert(playerdrop, v.Name)
end

function Tween(a)
local b = (a.Position - game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").Position).Magnitude
local speed = 150
tweenrach = game:GetService('TweenService'):Create(game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart"),TweenInfo.new(b/speed,Enum.EasingStyle.Linear), {CFrame=a})
tweenrach:Play()
if workspace.UncollectedSkins:FindFirstChild(a).TouchInterest == nil then
OrionLib:MakeNotification({
	Name = "Orion Hub",
	Content = "You already have that food skin!",
	Image = "rbxassetid://4483345998",
	Time = 3
})
end
end

function TweenTp(a)
local b = (a.Position - game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").Position).Magnitude
local speedtp = 100
tweenrach = game:GetService('TweenService'):Create(game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart"),TweenInfo.new(b/speedtp,Enum.EasingStyle.Linear), {CFrame=a})
tweenrach:Play()
end

