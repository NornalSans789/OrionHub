local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/thanhdat4461/OrionMoblie/main/source')))()

-- local linh tinh
local WS = game:GetService("Workspace")
local HMRP = game.Players.LocalPlayer.Character.HumanoidRootPart
local HM = game.Players.LocalPlayer.Character.Humanoid
local CHAR = game.Players.LocalPlayer.Character
local NAME = game.Players.LocalPlayer.Name
local player = game.Players:GetPlayers()
local WSPlayers = WS.Characters:FindFirstChild(NAME)

if game.PlaceId == 15240723287 then
OrionLib:MakeNotification({
	Name = "Script Loading...",
	Content = "Script has support your map: Secret Staycation",
	Image = "rbxassetid://4483345998",
	Time = 5
})
wait(0.15)
local Window = OrionLib:MakeWindow({Name = "Secret Staycation", HidePremium = false, SaveConfig = false, ConfigFolder = "OrionHub", IntroText = "Loading Hub..."})

-- local linh tinh :)
local playerdrop = {}
local speed = 175
getgenv().selectplr = nil
getgenv().selectstagetrain = nil
getgenv().selectplace = nil

-- function
for i,v in pairs(player) do
table.insert(playerdrop, v.Name)
end

spawn(function()
local k = Instance.new("Part")
k.Color = Color3.new(1,1,1)
k.Transparency = 1
k.Name = "Root"
k.Size = Vector3.new(1,1,1)
k.CanCollide = false
k.Anchored = true
k.Parent = game.Players.LocalPlayer.Character
wait(0.1)
game.Players.LocalPlayer.Character:FindFirstChild("Root").CFrame = HMRP.CFrame
wait(0.1)
local Root = game.Players.LocalPlayer.Character:FindFirstChild("Root")
end)

function Tween(a)
getgenv().Tween = true
local b = (a.p-game.Players.LocalPlayer.Character:FindFirstChild("Root").Position).Magnitude
tweenrach = game:GetService('TweenService'):Create(game.Players.LocalPlayer.Character:FindFirstChild("Root"),TweenInfo.new(b/speed,Enum.EasingStyle.Linear), {CFrame=a})
tweenrach:Play()
tweenrach.Completed:Connect(function()
getgenv().Tween = false
end)
game:GetService("RunService").Heartbeat:Connect(function()
if Tween == true then
HMRP.CFrame = game.Players.LocalPlayer.Character:FindFirstChild("Root").CFrame
end
end)
if game.Players.LocalPlayer.Character.Humanoid.Health == 0 then
while wait() do
game.Players.LocalPlayer.Character:FindFirstChild("Root").CFrame = HMRP.CFrame
end
end
end

