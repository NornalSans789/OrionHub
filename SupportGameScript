local TpTab = Window:MakeTab({
	Name = "Tab Teleport",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local StageTrainSection = TpTab:AddSection({
	Name = "Stage Train Section"
})

TpTab:AddDropdown({
	Name = "Select Stage Toy Train",
	Default = "1",
	Options = {"1", "2", "3", "4", "5"},
	Callback = function(Value)
	selectstagetrain = Value
	end    
})

TpTab:AddButton({
	Name = "Tp To Stage Train",
	Callback = function()
     TweenTp(workspace.ToyTrain[selectstagetrain].CFrame)
  	end    
})

local HouseSection = TpTab:AddSection({
	Name = "House Place Section"
})

TpTab:AddDropdown({
	Name = "Select Place",
	Default = "...",
	Options = {"Cutting boards", "Skating area", "Top Refrigerator", "Sofa", "Front Tv", "Behide Tv", "Dinner table", "Game table", "Bookshelf", "Top Bookshelf", "Mysterious tunnel", "Chairs", "Air-conditioner", "Pots", "Styrofoam box", "Frames", "Washing machine", "Hand sink", "Body lotion frame"},
	Callback = function(Value)
	selectplace = Value
	end    
})

TpTab:AddButton({
	Name = "Tp to Place",
	Callback = function()
if selectplace == "..." then
OrionLib:MakeNotification({
	Name = "Orion Hub",
	Content = "Please select a location to go there",
	Image = "rbxassetid://4483345998",
	Time = 3
})
elseif selectplace == "Cutting boards" then
Tween(CFrame.new(-167.2939453125, 43.239356994628906, -51.28073501586914))
elseif selectplace == "Skating area" then
Tween(CFrame.new(-68.79549407958984, -6.015318870544434, 124.84772491455078))
elseif selectplace == "Top Refrigerator" then
Tween(CFrame.new(-5.823515892028809, 85.33843231201172, -89.01644134521484))
elseif selectplace == "Sofa" then
Tween(CFrame.new(52.188228607177734, 18.655014038085938, 194.65689086914062))
elseif selectplace == "Front Tv" then
Tween(CFrame.new(168.59326171875, 18.238250732421875, 196.1866455078125))
elseif selectplace == "Behide Tv" then
Tween(CFrame.new(180.7163543701172, 18.287065505981445, 197.2242889404297))
elseif selectplace == "Dinner table" then
Tween(CFrame.new(-12.043829917907715, 42.9599494934082, 25.65976333618164))
elseif selectplace == "Game table" then
Tween(CFrame.new(119.70698547363281, 14.137039184570312, 193.68919372558594))
elseif selectplace == "Bookshelf" then
Tween(CFrame.new(172.07998657226562, 55.58022689819336, -9.5766019821167))
elseif selectplace == "Top Bookshelf" then
Tween(CFrame.new(170.0970001220703, 93.05994415283203, 0.6084963083267212))
elseif selectplace == "Mysterious tunnel" then
Tween(CFrame.new(180.4864044189453, -4.153239727020264, -11.73713207244873))
elseif selectplace == "Chairs" then
Tween(CFrame.new(-33.910770416259766, 23.065053939819336, 19.025802612304688))
elseif selectplace == "Air-conditioner" then
Tween(CFrame.new(126.79120635986328, 117.18878173828125, 314.5452575683594))
elseif selectplace == "Pots" then
Tween(CFrame.new(-117.59017181396484, 16.126821517944336, 384.9138488769531))
elseif selectplace == "Styrofoam box" then
Tween(CFrame.new(-168.95677185058594, 86.14543914794922, 300.3447570800781))
elseif selectplace == "Frames" then
Tween(CFrame.new(-15.589682579040527, 95.10487365722656, 314.89306640625))
elseif selectplace == "Washing machine" then
Tween(CFrame.new(-444.3197937011719, 8.307300567626953, 296.6883850097656))
elseif selectplace == "Hand sink" then
Tween(CFrame.new(-386.2815246582031, 31.28902816772461, 300.7383728027344))
elseif selectplace == "Body lotion frame" then
Tween(CFrame.new(-243.6700897216797, 67.58553314208984, 297.29620361328125))
end
  	end    
})

local PlayerTab = Window:MakeTab({
	Name = "Tab Player",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local PlayerSection = PlayerTab:AddSection({
	Name = "View Section"
})

PlayerTab:AddDropdown({
	Name = "Select Player",
	Default = game.Players.LocalPlayer.Name,
	Options = playerdrop,
	Callback = function(Value)
	selectplr = Value
	end    
})

PlayerTab:AddButton({
	Name = "Tp Player",
	Callback = function()
    TweenTp(game.Players:FindFirstChild(selectplr).Character.HumanoidRootPart.CFrame)
  	end    
})

PlayerTab:AddToggle({
	Name = "Spectator Player",
	Default = false,
	Callback = function(Value)
getgenv().spectator = Value

if spectator == true then
game.Workspace.CurrentCamera.CameraSubject = game.Players:FindFirstChild(selectplr).Character
else
game.Workspace.CurrentCamera.CameraSubject = game.Players.LocalPlayer.Character
end
	end    
})
else
OrionLib:MakeNotification({
	Name = "Errors!!!",
	Content = "Script current has not support your current map!",
	Image = "rbxassetid://4483345998",
	Time = 5
})
end 