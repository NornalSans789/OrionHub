local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/thanhdat4461/OrionMoblie/main/source')))()
OrionLib:MakeNotification({
	Name = "Orion Hub!",
	Content = "Checking game..",
	Image = "rbxassetid://4483345998",
	Time = 2.5
})
if game.PlaceId == 15068118098 then
wait(3)
OrionLib:MakeNotification({
	Name = "Welcome, "..game.Players.LocalPlayer.Name,
	Content = "You are now Playing Super Toilet Brawl!",
	Image = "rbxassetid://4483345998",
	Time = 2.5
})

OrionLib:MakeNotification({
	Name = "Remember!",
	Content = "You must choose a large morph or a buffalo to farm without worrying about dying",
	Image = "rbxassetid://4483345998",
	Time = 5
})

local player = game.Players.LocalPlayer
local Player = game.Players.LocalPlayer
local Window = OrionLib:MakeWindow({Name = "Super Toilet Brawl", HidePremium = false, SaveConfig = false, ConfigFolder = "SuperToilet"})

while getgenv().c == true do
wait(.1)
autofarm()
end

-- all mode
function autofarm()
toilet()
massivetoilet()
bigtoilet()
bosstoilet()
titantoilet()
gianttoilet()
corrupted()
end

function massivetoilet()
game:GetService("VirtualUser"):ClickButton1(Vector2.new(9e9, 9e9))
if game.Workspace.Toilets:FindFirstChild("Massive Toilet") then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace.Toilets["Massive Toilet"].HumanoidRootPart.CFrame * CFrame.new(-1, -2, 3)
if game.Workspace.Toilets:FindFirstChild("Massive Glasses Toilet") then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace.Toilets["Massive Glasses Toilet"].HumanoidRootPart.CFrame * CFrame.new(-1, -2, 3)
if game.Workspace.Toilets:FindFirstChild("Massive Armorred Toilet") then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace.Toilets["Massive Armorred Toilet"].HumanoidRootPart.CFrame * CFrame.new(-1, -2, 3)
if game.Workspace.Toilets:FindFirstChild("Massive Metal Toilet") then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace.Toilets["Massive Metal Toilet"].HumanoidRootPart.CFrame * CFrame.new(-1, -2, 3)
game:GetService("VirtualUser"):ClickButton1(Vector2.new(9e9, 9e9))
game:GetService("VirtualUser"):ClickButton1(Vector2.new(9e9, 9e9))
end
end
end
end
end

