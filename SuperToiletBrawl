-- Code Gui
local MainTab = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local MainSection = MainTab:AddSection({
	Name = "Main Section"
})

MainTab:AddToggle({
	Name = "God Mode [Visual]",
	Default = false,
	Callback = function(text)
getgenv().Safe = text

if getgenv().Safe == true then
OrionLib:MakeNotification({
	Name = "Warning!",
	Content = "This is just visualization",
	Image = "rbxassetid://4483345998",
	Time = 2.5
})
while getgenv().Safe == true do
wait(.1)
local HP = game.Players.LocalPlayer.Character.Humanoid.MaxHealth
game.Players.LocalPlayer.Character.Humanoid.Health = HP
game.Players.LocalPlayer.Character.Humanoid.MaxHealth = 9999999999
end
end
	end    
})

MainTab:AddButton({
        Name = "Speed Boost [Bypass]",
        Callback = function()
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed += 25
          end    
})

MainTab:AddButton({
        Name = "Jump Boost [Bypass]",
        Callback = function()
        game.Players.LocalPlayer.Character.Humanoid.JumpPower += 25
          end    
})

local FarmTab = Window:MakeTab({
	Name = "Farm [Beta]",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local FarmSection = FarmTab:AddSection({
	Name = "Farm Section"
})

FarmTab:AddToggle({
	Name = "Auto Farm",
	Default = false,
	Callback = function(vl)
getgenv().c = vl

if getgenv().c == true then
OrionLib:MakeNotification({
	Name = "Farming!",
	Content = "Started farming the current mode!",
	Image = "rbxassetid://4483345998",
	Time = 2.5
})
OrionLib:MakeNotification({
	Name = "Remember!",
	Content = "Wave 20 or wave 30 or Wave 35 or Wave 40 requires 1 or 2 person to be able to farm without losing the mode when dying",
	Image = "rbxassetid://4483345998",
	Time = 5
})
end
	end    
})

local CreditTab = Window:MakeTab({
	Name = "Credit",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local CreditSection = CreditTab:AddSection({
	Name = "Credit Section"
})

CreditTab:AddLabel("By @masket_smile6013(Youtube)")

local CreditRemakeSection = CreditTab:AddSection({
	Name = "Credit Remake Section"
})
CreditTab:AddLabel("By @SansYT789(Youtube)")

else
OrionLib:MakeNotification({
	Name = "Orion Hub!",
	Content = "You are now Playing Not Supported game! Sorry.",
	Image = "rbxassetid://4483345998",
	Time = 2.5
})
OrionLib:Init()
end
